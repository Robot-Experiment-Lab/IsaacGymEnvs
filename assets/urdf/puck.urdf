<?xml version="1.0"?>
<robot name="puck">
  <link name="puck">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.06" length="0.03"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0"/>
      <!-- inertia of cyclinder m=0.1 kg r=0,06 m l= 0.03 m -->
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0002"/>

    </inertial>
  </link>
</robot>
